<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
       <settings>
              <setting name="cacheEnabled" value="false" />
              <!-- 全局启用或禁用延迟加载。当禁用时，所有关联对象都会即时加载-->
              <setting name="lazyLoadingEnabled" value="true" />
              <setting name="multipleResultSetsEnabled" value="true" />
              <setting name="useColumnLabel" value="true" />
              <setting name="defaultExecutorType" value="REUSE" />
              <setting name="defaultStatementTimeout" value="25000" />
              <setting name="mapUnderscoreToCamelCase" value="true" />
       </settings>
       <typeAliases>
              <!--<typeAlias alias="DropDownItem" type="com.botech.skynet.comm.qvo.DropDownItem" />-->
              <package name="com.botech.skynet.base.domain" />
              <package name="com.botech.skynet.index.dto" />
              <package name="com.botech.skynet.index.qvo" />
              <package name="com.botech.skynet.sysmng.dto" />
              <package name="com.botech.skynet.sysmng.qvo" />
              <package name="com.botech.skynet.upload.dto" />
              <package name="com.botech.skynet.upload.qvo" />
       </typeAliases>
</configuration>